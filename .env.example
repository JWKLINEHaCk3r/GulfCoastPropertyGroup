# API KEYS & SECRETS
OPENAI_API_KEY=your_openai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here

# DATABASE
DATABASE_URL=postgresql://user:password@localhost:5432/realestate
REDIS_URL=redis://localhost:6379/0

# THIRD-PARTY INTEGRATIONS
DOCUSIGN_API_KEY=your_docusign_key
DOCUSIGN_ACCOUNT_ID=your_account_id
TWILIO_ACCOUNT_SID=your_twilio_sid
TWILIO_AUTH_TOKEN=your_twilio_token
TWILIO_PHONE_NUMBER=+1234567890

# ZILLOW/REDFIN
ZILLOW_API_KEY=optional_key
REDFIN_API_KEY=optional_key

# SENDGRID
SENDGRID_API_KEY=your_sendgrid_key
SENDGRID_FROM_EMAIL=noreply@realestate.com

# APP CONFIG
ENVIRONMENT=development
DEBUG=True
SECRET_KEY=your-secret-key-change-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# BUSINESS CONFIG
MIN_LEAD_SCORE_THRESHOLD=65
WHOLESALE_FEE_PERCENTAGE=6
DEFAULT_OFFER_DISCOUNT_PERCENT=30

# SCRAPING & RATE LIMITS
SCRAPER_DELAY_SECONDS=2
MAX_REQUESTS_PER_MINUTE=60
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36

# FEATURE FLAGS
ENABLE_LEAD_SCOUT=True
ENABLE_OFFER_GENERATION=True
ENABLE_BUYER_MATCHING=True
ENABLE_NEGOTIATION_BOT=True
ENABLE_SEO_AUTOMATION=True
